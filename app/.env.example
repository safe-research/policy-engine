# Policy Engine Configuration
# Copy this file to .env and update the values

# Cosigner address for fallback policy
# This address can co-sign transactions when other policies don't match
VITE_COSIGNER_ADDRESS=0x0000000000000000000000000000000000000000

# Base URL for the application (used for deployment)
# Use "/" for root deployment, or "/subdirectory/" for subdirectory deployment
VITE_BASE_URL='/policy-engine/'
